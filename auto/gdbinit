set architecture i386:intel
set print pretty

add-symbol-file bin/symbols/BM.elf 0x1000
#add-symbol-file bin/kernel/kernel.elf 0x100000
add-symbol-file bin/kernel/kernel.elf
add-symbol-file usr/fs_layout/sys/terminal.elf

#layout prev
layout src

target remote localhost:1234

# MBR start
#break *0x7c00

# Bootmain start
#break bootmain

# Kernel physical entry
#break _start

# Kernel main start
break kernel_main
#break _entry

#break init_fs

#break pm_load
#break interrupt_handler
#break ui_tab_open
# break pmm_free_page
# break vmm_del_ctx
# break __gdb_break
break ui_stdout_write
# break ui_key_event_handler

continue